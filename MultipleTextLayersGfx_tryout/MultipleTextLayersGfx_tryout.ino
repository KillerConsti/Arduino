bool WriteLetter(SMLayerGFXMono<rgb24, rgb1, 0> *layer,char Letter)
{
  //look for umlauts
  //notice if we use umlauts we loose one char as it casts a *195*
  int num =static_cast<int>(Letter);
  if(num == 195)
  {
    return false;
  }
  if(num == 164) //
  {
    layer->write(0x84);
    return true;
  }
      if(num == 182) //
  {
    layer->write(0x94);
    return true;
  }
    if(num == 188) //
  {
    layer->write(0x81);
    return true;
  }
  
  layer->print(Letter);
  return true;
}
